	"reply-mode": "thread",
	"stream-depth": 5,
	"image-format": "png",
	"appends-css": false,
	"appends-rss": false,
	"counts-deletions": false,
	"local-metadata": false,
	"stores-ip-address": false,
	"minifies-javascript": false,
	"minify-level": 1
