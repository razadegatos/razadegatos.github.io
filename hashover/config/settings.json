{
	"language": "en-us",
	"theme": "default-borderless",
	"default-sorting": "threaded-by-date",
	"uses-markdown": true,
	"uses-ajax": true,
	"shows-reply-count": true,
	"allows-images": true,
	"allows-likes": true,
	"allows-dislikes": false,
	"uses-moderation": false,
	"pends-user-edits": false,
	"mail-type": "text",
	"mailer": "sendmail",
	"subscribes-user": true,
	"allows-user-replies": false,
	"sets-cookies": true,
	"cookie-expiration": "next month",
	"secure-cookies": false,
	"collapses-interface": false,
	"collapses-comments": false,
	"collapse-limit": 3,
	"popularity-threshold": 5,
	"popularity-limit": 2,
	"spam-database": "remote",
	"spam-check-modes": "both",
	"icon-mode": "count",
	"icon-size": 45,
	"gravatar-default": "custom",
